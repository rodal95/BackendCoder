<div>

    <h1>Tabla</h1>
  
    <table class="table">
  <thead>
    <tr>
      
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
   {{#each productos}}
        <tr>
          <td>{{this.name}}</td>
          <td>{{this.price}}</td>
          <td><img src="{{this.image}}" width="100px"></td>
        </tr>
         {{/each}}
    
  </tbody>
</table>
<a href="/"><button class="btn btn-primary">Home</button></a>
<a href="/formulario"><button class="btn btn-primary">Formulario</button></a>
</div>